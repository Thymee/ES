  /*超小设备（手机，小于768px)*/
@media (max-width: 767px){
	.lanwon-viewer,.toolbar-top{
	display:none;
	} 
	.thumbnailSelector{
		margin-left: -999px;
	}
	
}

   /*小型设备（平板电脑，768px起）*/
@media (min-width: 767px) and (max-width: 1180px) {
  .lanwon-viewer,.large-screen{
		display: none;
	}   
}


  /*中型设备（台式电脑，992px~1200px）*/ 
@media (min-width: 1180px) and (max-width: 1350px) {
  	.lanwon-viewer,.ipad-screen{
		display: none;
	}   
}

   /*大型设备（大台式电脑，1200px起）*/
@media (min-width: 1350px) {
	.ipad-screen{
		display: none;
	}
	
}


.progress-bar-color{
	background-color:#383842;
}


/*放大镜工具*/
.magnifyTool {
	width: 225px;
	height: 225px;
    border: 4px white solid;
    box-shadow: 2px 2px 10px #1e1e1e;
    border-radius: 50%;
    display: none;
    cursor: none;
}


/*箭头标注*/
/* prevents selection when left click dragging */
        .disable-selection {
            -moz-user-select: none; -webkit-user-select: none; -ms-user-select:none; user-select:none;
        }
        /* prevents cursor from changing to the i bar on the overlays*/
        .noIbar {
            cursor:default;
        }
        .annotationDialog, .relabelDialog {
            z-index: 1000;
            position: absolute;
            margin: 0px;
            left: 40%;
            top: 40%;
            width: 300px;
            border: 1px black solid;
            border-radius: 5px;
        }
        .annotationTextInputOptions {
            padding: 5px 0px;
        }
        .annotationTextInput {
            margin-left: 5px;
        }
        /*.annotationDialogConfirm {
            float: right;
        }*/
        


.annotationDialog h5, .relabelDialog h5{

    font-size: 14px;
    margin-top: 10px;
    margin-bottom: 10px;
    line-height: 1.1;
	
}

.annotationDialog .annotationTextInput,.relabelDialog .annotationTextInput{
    font: 14px/1.42857143 arial, \5b8b\4f53;
}

dialog a{
	margin: 5px;
}
.viewer {
    /*position: absolute;left: 110px;width: 100%;*/
    float: left;
    box-sizing: border-box;
}

.thumbnailSelector {
    float:left;
    margin-left:0px;
    height: 100%;
    overflow-x: hidden;
    overflow-y: scroll;
    background-color: #383842;
}
/*下载序列影像*/
a.list-group-item{
	overflow: hidden;
}
.downLoadSeries{
	/*display:none;*/
	/*opacity: 0;*/
	position: absolute;
  top: -22px;
  right: 0;
  height: 22px;
  line-height: 22px;
  transition: all .2s;
  width: 100%;
  background-color:#48494e;
}
 
.downLoadSeries-down{
	margin:0 auto;
	width:100px;
	border-radius:2px;
	font-size:14px;
	color:#e6e6ef;
}
.downLoadSeries-down:hover{ 
	color:#3c8fed;
}
