/* reset */

@font-face {font-family: 'SourceHanSans-Normal';
  src: url('//at.alicdn.com/t/webfont_i1x9qdvv9veipb9.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_i1x9qdvv9veipb9.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_i1x9qdvv9veipb9.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_i1x9qdvv9veipb9.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_i1x9qdvv9veipb9.svg#NotoSansHans-DemiLight') format('svg'); /* iOS 4.1- */
}
@font-face {font-family: 'SourceHanSans-Regular';
  src: url('//at.alicdn.com/t/webfont_3zbo8eop57ory66r.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_3zbo8eop57ory66r.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_3zbo8eop57ory66r.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_3zbo8eop57ory66r.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_3zbo8eop57ory66r.svg#思源黑体-普通') format('svg'); /* iOS 4.1- */
}

html,body,h1,h2,h3,h4,h5,h6,div,dl,dt,dd,ul,ol,li,p,blockquote,pre,hr,figure,table,caption,th,td,form,fieldset,legend,input,button,textarea,menu {
	margin: 0;
	padding: 0;
}

header,footer,section,article,aside,nav,hgroup,address,figure,figcaption,menu,details {
	display: block;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

caption,
th {
	text-align: left;
	font-weight: normal;
}

html,body,fieldset,img,iframe,abbr {
	border: 0;
}

i,cite,em,var,address,dfn {
	font-style: normal;
}

[hidefocus],
summary {
	outline: 0;
}

li {
	list-style: none;
}

h1,h2,h3,h4,h5,h6,small {
	font-size: 100%;
}

sup,sub {
	font-size: 83%;
}

pre,code,kbd,samp {
	font-family: inherit;
}

q:before,
q:after {
	content: none;
}

textarea {
	overflow: auto;
	resize: none;
}

label,summary {
	cursor: default;
}

a,button {
	cursor: pointer;
}

h1,h2,h3,h4,h5,h6,em,strong,b {
	font-weight: bold;
}

del,ins,u,s,a,a:hover {
	text-decoration: none;
}

body,textarea,input,button,select,keygen,legend {
	font: 12px/1.14 arial, \5b8b\4f53;
	/*color: #333;*/
	outline: 0;
}

body {
	background: #fff;
}

a,
a:hover {
	color: #333;
}
.left{
	float: left;
}
.right{
	float: right;
}
/* 全屏自适应布局  */

html,
body {
	width: 100%;
	height: 100%;
	overflow: hidden;
	margin: 0;
	background: #333;
	font-family: "SourceHanSans-Regular", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}

/*目录树*/

.tree {
	color: #ccc;
	width: 156px;
	margin:14px auto 0 auto;
	font-size: 13px;
}

/*自定义滚动条*/
 ::-webkit-scrollbar{
 width: 16px;
 background:#181818 ;
 }  
 ::-webkit-scrollbar-button:start{
 background:url(../images/scrollbtnT.png) ;
 }  
  ::-webkit-scrollbar-button:end{
 background:url(../images/scrollbtnB.png) ;
 } 
 ::-webkit-scrollbar-thumb{
 background: #5a5a5a;
 }   
 Body {
scrollbar-arrow-color: #66686c;  /*图6,三角箭头的颜色
scrollbar-face-color: #5a5a5a;  /*图5,立体滚动条的颜色*/
scrollbar-3dlight-color: #5a5a5a;  /*图1,立体滚动条亮边的颜色*/
scrollbar-highlight-color: #181818;  /*图2,滚动条空白部分的颜色*/
scrollbar-track-color: #181818;  /*图7,立体滚动条背景颜色*/
scrollbar-base-color:#5a5a5a;  /*滚动条的基本颜色*/
Cursor:url(mouse.cur);  /*自定义个性鼠标*/
}    
